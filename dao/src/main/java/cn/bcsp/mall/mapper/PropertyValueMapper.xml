<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 配置命名空间，区别名称 -->

<mapper namespace="cn.bcsp.mall.mapper.PropertyValueMapper">

    <select id="selectAll" parameterType="int" resultType="cn.bcsp.mall.pojo.PropertyValue">
        select * from propertyvalue where propertyValueProductId=#{productId}
    </select>

</mapper>